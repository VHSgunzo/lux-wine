check_vsync() {
    if [ ! -z "$1" ] ; then export VSYNC="$1" ; fi
    case "$VSYNC" in
        Enabled) export vblank_mode=1
                 export __GL_SYNC_TO_VBLANK=1
                 print_info notify "Vsync in force enabled mode!" ;;
        Disabled) unset vblank_mode
                  unset __GL_SYNC_TO_VBLANK ;;
    esac
}

